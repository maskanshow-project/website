{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "askedio/laravel-soft-cascade": "^5.7",
        "awssat/laravel-visits": "^1.5",
        "cviebrock/eloquent-sluggable": "^4.6",
        "cybercog/laravel-likeable": "^3.1",
        "dimsav/laravel-translatable": "^9.4",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "~2.0",
        "grimzy/laravel-mysql-spatial": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "larabook/gateway": "^3.3",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "morilog/jalali": "3.*",
        "nicolaslopezj/searchable": "^1.10",
        "owen-it/laravel-auditing": "^8.0",
        "php-http/message": "^1.7",
        "pragmarx/tracker": "^3.3",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.4",
        "rebing/graphql-laravel": "^1.20",
        "santigarcor/laratrust": "5.2.*",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-tags": "^2.2",
        "tucker-eric/eloquentfilter": "^1.4",
        "vinkla/instagram": "^9.3",
        "ybazli/faker": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
